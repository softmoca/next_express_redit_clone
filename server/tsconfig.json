{
  "compilerOptions": {
    "target": "es2016",

    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,

    /* Modules */
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
